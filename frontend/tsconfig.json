{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "types": ["cypress"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src", ".", "cypress/**/*.ts", "cypress/**/*.tsx"],
  "exclude": ["node_modules", "dist"]
}
